    // Features Start
    section.features.space-pb-100
      .container
        +title('Features','Benifits Of Yoga')       
        .row.cdx-row
          .col-md-6.col-lg-4(data-sal-delay="100" data-sal="slide-up" data-sal-duration="800")
            +featuregrid('Addition of Energy','1.png')
          .col-md-6.col-lg-4(data-sal-delay="200" data-sal="slide-up" data-sal-duration="800")
            +featuregrid('Renewing of Mind','2.png')
          .col-md-6.col-lg-4(data-sal-delay="300" data-sal="slide-up" data-sal-duration="800")
            +featuregrid('Create cool mind','3.png')
          .col-md-6.col-lg-4(data-sal-delay="100" data-sal="slide-up" data-sal-duration="800")
            +featuregrid('Quality Trainers','4.png')
          .col-md-6.col-lg-4(data-sal-delay="200" data-sal="slide-up" data-sal-duration="800")
            +featuregrid('Stretch Body','5.png')       
          .col-md-6.col-lg-4(data-sal-delay="300" data-sal="slide-up" data-sal-duration="800")
            +featuregrid('Improves Body Posture','6.png')
    // Features End


    // Counter Start
    +countergrid
    // Counter End

    // Class Start
    section.space-py-100
      .container
        +title('CLASSES','Our Best Classes')       
        .row.cdx-row
          .col-md-6.col-xl-4(data-sal-delay="100" data-sal="slide-up" data-sal-duration="800")
            +classgrid('$129','Yoga For Beginners','Ruth Edwards','1.jpg','1.jpg')
          .col-md-6.col-xl-4(data-sal-delay="200" data-sal="slide-up" data-sal-duration="800")
            +classgrid('$99','Balance Body & Mind','Omini Green','2.jpg','2.jpg')
          .col-md-6.col-xl-4(data-sal-delay="300" data-sal="slide-up" data-sal-duration="800")
            +classgrid('$119','Yoga Classes For Children','Raymond Gill','3.jpg','3.jpg')
          .col-md-6.col-xl-4(data-sal-delay="100" data-sal="slide-up" data-sal-duration="800")
            +classgrid('$82','Increased Body Awareness','Sarah Albert','4.jpg','4.jpg')
          .col-md-6.col-xl-4(data-sal-delay="200" data-sal="slide-up" data-sal-duration="800")
            +classgrid('$126','Hatha Yoga','Zahra Sharif','5.jpg','5.jpg')
          .col-md-6.col-xl-4(data-sal-delay="300" data-sal="slide-up" data-sal-duration="800")
            +classgrid('$105','Pilates Yoga','Miranda Doe','6.jpg','6.jpg')
    // Class End


    // schedule Start
    section.time-schedule(data-sal-delay="100" data-sal="slide-up" data-sal-duration="800")
      .container
        .row
          +title('Schedule','Our Time Schedule')         
          .col-12(data-sal-delay="100" data-sal="slide-up" data-sal-duration="800")
            +timesedual
      ul.cdx-shap
        li
          img.img-fluid(src='../assets/images/shap/6.png' alt='')
    // schedule Start


